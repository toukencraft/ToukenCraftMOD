{
	"schemaVersion": 1,
	"id": "toukencraft",
	"version": "${version}",

	"name": "ToukenCraft",
	"description": "A mod that summons Touken Danshi into the world of Minecraft",
	"authors": ["ToukenCraft"],
	"contact": {
		"email": "touken.craft@gmail.com",
		"sources": "https://github.com/toukencraft/ToukenCraftMOD",
		"issues": "https://github.com/toukencraft/ToukenCraftMOD/issues"
	},

	"license": "MIT",
	"icon": "assets/toukencraft/icon.png",

	"environment": "*",
	"entrypoints": {
		"client": [
			"com.github.toukencraft.toukencraft.client.ToukenCraftClient"
		],
		"main": [
			"com.github.toukencraft.toukencraft.ToukenCraft"
		],
        "fabric-datagen": [
            "com.github.toukencraft.toukencraft.ToukenCraftDataGenerator"
        ]
	},
	"mixins": [
		"toukencraft.mixins.json"
	],
	"accessWidener": "toukencraft.accesswidener",

	"depends": {
		"fabricloader": ">=0.16.14",
		"fabric-api": "*",
		"minecraft": ">=1.21.9",
		"java": ">=21"
	},
	"suggests": {

	}
}