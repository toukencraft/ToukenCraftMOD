{
  "schemaVersion": 1,
  "id": "toukencraft",
  "version": "${version}",

  "name": "ToukenCraft",
  "description": "A mod that summons Touken Danshi into the world of Minecraft",
  "authors": ["ToukenCraft"],
  "contact": {
    "email": "touken.craft@gmail.com"
  },

  "license": "MIT",
  "icon": "assets/toukencraft/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "com.github.toukencraft.toukencraft.client.ToukenCraftClient"
    ],
    "main": [
      "com.github.toukencraft.toukencraft.ToukenCraft"
    ],
    "fabric-datagen": [
      "com.github.toukencraft.toukencraft.ToukenCraftDataGenerator"
    ]
  },

  "mixins": [
    "toukencraft.mixins.json"
  ],
    
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
